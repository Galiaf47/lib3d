{
  "name": "lib3d",
  "version": "0.0.2",
  "description": "Display 3D library on your webpage",
  "main": "./dist/lib3d.js",
  "scripts": {
    "push": "git push origin HEAD --follow-tags",
    "dev": "webpack-dev-server",
    "test": "karma start --single-run",
    "test-watch": "karma start",
    "esdoc": "esdoc -c ./esdoc.json",
    "jsdoc": "jsdoc -c ./jsdoc.json",
    "deploy": "npm run build && npm publish",
    "build": "webpack --progress --colors",
    "postpublish": "npm run esdoc-publish && npm run jsdoc-publish",
    "esdoc-publish": "esdoc-uploader",
    "jsdoc-publish": "./jsdoc-publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Galiaf47/lib3d.git"
  },
  "keywords": [
    "library",
    "3d",
    "bookshelf",
    "books"
  ],
  "author": {
    "name": "Siarhei Klimuts",
    "email": "galiaf47@gmail.com"
  },
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/Galiaf47/lib3d/issues"
  },
  "homepage": "https://github.com/Galiaf47/lib3d#readme",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.3.13",
    "esdoc": "^0.4.6",
    "esdoc-uploader": "^1.0.1",
    "file-loader": "^0.8.5",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "jsdoc": "^3.4.0",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-read-json": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lib3d-objects": "0.0.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "shader-loader": "^1.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "three": "^0.72.0"
  }
}
